{
  "name": "scs-js",
  "version": "0.1.0",
  "description": "Implementation of scs-language tools for web",
  "main": "index.js",
  "scripts": {
    "build": "npm run compile && webpack --progress --colors",
    "compile": "cd src/syntax && antlr4ts -no-listener scs.g4",
    "dev": "tsc -watch -p ./",
    "test": "npm run compile && mocha -r ts-node/register test/*.ts",
    "watch": "webpack --progress --colors --watch"
  },
  "keywords": [
    "ostis",
    "scs"
  ],
  "author": "Denis Koronchik <denis.koronchik@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@ostis/sc-core": "^0.1.3",
    "@types/mocha": "^2.2.44",
    "antlr4": "^4.7.0",
    "antlr4ts": "^0.4.1-alpha.0"
  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "antlr4ts-cli": "^0.4.0-alpha.4",
    "awesome-typescript-loader": "^3.2.3",
    "chai": "^4.1.2",
    "mocha": "^4.0.1",
    "source-map-loader": "^0.2.3",
    "ts-loader": "^2.3.7",
    "ts-node": "^3.3.0",
    "tsc": "^1.20150623.0",
    "tslint": "^5.8.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.6.1",
    "webpack": "^2.7.0",
    "yargs": "^8.0.2"
  }
}
